#. Description
#: ../partman-partitioning.templates:194
msgid "Name:"
msgstr "Nome:"

#. Type: text
#. Description
#: ../partman-partitioning.templates:198
msgid "Bootable flag:"
msgstr "Flag inicializável:"

#. Type: text
#. Description
#: ../partman-partitioning.templates:202
msgid "on"
msgstr "ligado"

#. Type: text
#. Description
#: ../partman-partitioning.templates:206
msgid "off"
msgstr "desligado"

