#. Type: text
#. Description
#. :sl1:
#: ../partman-base.templates:49001
#, no-c-format
msgid "SCSI%s (%s)"
msgstr "SCSI%s (%s)"

#. Type: text
#. Description
#. :sl1:
#: ../partman-base.templates:50001
#, no-c-format
msgid "SCSI%s, partition #%s (%s)"
msgstr "SCSI%s, phân vùng #%s (%s)"

#. Type: text
#. Description
#. For example MMC/SD card #1 (mmcblk0)
#. :sl3:
#: ../partman-base.templates:51001
#, no-c-format
msgid "MMC/SD card #%s (%s)"
