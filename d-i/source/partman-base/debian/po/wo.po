msgstr "Loopback (loop%s)"

#. Type: text
#. Description
#: ../partman-base.templates:262
#, no-c-format
msgid "DASD %s (%s)"
msgstr "DASD %s (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:266
#, no-c-format
msgid "DASD %s (%s), partition #%s"
msgstr "DASD %s (%s), partisoŋ #%s"

#. Type: text
#. Description
#: ../partman-base.templates:270
msgid "Cancel this menu"
msgstr "Neenal bii menu"

